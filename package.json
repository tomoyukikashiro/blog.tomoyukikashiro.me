{
  "name": "regular",
  "private": true,
  "version": "1.0.0",
  "engines": {
    "node": "12.x"
  },
  "dependencies": {
    "gatsby": "^2.21.9",
    "gatsby-plugin-feed": "^2.5.0",
    "gatsby-plugin-html2amp": "^1.3.3",
    "gatsby-plugin-react-helmet": "^3.3.0",
    "gatsby-plugin-react-svg": "3.1.0",
    "gatsby-plugin-sass": "^2.3.0",
    "gatsby-plugin-sharp": "^2.6.0",
    "gatsby-plugin-sitemap": "^2.4.1",
    "gatsby-plugin-webpack-bundle-analyzer": "1.0.5",
    "gatsby-remark-autolink-headers": "^2.3.0",
    "gatsby-remark-copy-linked-files": "^2.3.1",
    "gatsby-remark-images": "^3.3.0",
    "gatsby-remark-prismjs": "^3.5.0",
    "gatsby-remark-responsive-iframe": "^2.4.1",
    "gatsby-remark-smartypants": "^2.3.0",
    "gatsby-source-filesystem": "^2.3.0",
    "gatsby-transformer-remark": "^2.8.4",
    "gatsby-transformer-sharp": "^2.5.0",
    "immutable": "3.8.2",
    "node-sass": "^4.14.0",
    "prismjs": "^1.20.0",
    "prop-types": "15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "remark-breaks": "^1.0.5",
    "reset-css": "5.0.1",
    "slate": "^0.57.2",
    "typeface-merriweather": "0.0.72",
    "typeface-montserrat": "0.0.75"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "amphtml-validator": "^1.0.30",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.5.1",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-preset-gatsby": "^0.4.0",
    "eslint": "6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "^25.5.4",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1"
  },
  "scripts": {
    "dev": "npm run build:utils && gatsby develop",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "amp": "amphtml-validator public/post/**/*.html && amphtml-validator public/index.html",
    "unit": "jest",
    "test": "npm run lint && npm run unit && npm run amp",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "develop": "npm run build:utils && gatsby develop",
    "build": "npm run build:utils && gatsby build --log-pages",
    "serve": "gatsby serve",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js",
    "build:utils": "babel src/utils/post.js --out-file src/utils/dist/post.js --config-file ./src/utils/.babelrc && babel src/utils/site.js --out-file src/utils/dist/site.js --config-file ./src/utils/.babelrc"
  }
}
